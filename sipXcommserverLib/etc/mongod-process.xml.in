<?xml version='1.0' encoding='iso-8859-1' standalone='yes'?>
<sipXecs-process xmlns='http://www.sipfoundry.org/sipX/schema/xml/sipXecs-process-01-00'>
  <name>mongod</name>
  <version>@VERSION@</version>
  <commands>
    <configtest>
      <execute>@SIPX_BINDIR@/sipxmongod</execute>
      <parameter>--configtest</parameter>
    </configtest>
    <start>
      <execute>@SIPX_BINDIR@/sipxmongod</execute>
    </start>
    <stop>
      <execute>@SIPX_BINDIR@/sipxmongod</execute>
      <parameter>--stop</parameter>
    </stop>
  </commands>
  <status>
    <pid>@SIPX_RUNDIR@/sipxmongod.pid</pid>
    <log>@SIPX_LOGDIR@/sipxmongod.log</log>
  </status>
  <resources>
    <file>@SIPX_CONFDIR@/mongo/mongod.conf</file>
  </resources>
</sipXecs-process>
