.PHONY: all all-am check check-am clean clean-generic clean-libtool \
	distclean distclean-generic distclean-libtool distdir dvi \
	dvi-am html html-am info info-am install install-am \
	install-data install-data-am install-data-hook \
	install-dist_authoritiesDATA install-dvi install-dvi-am \
	install-exec install-exec-am install-exec-hook install-html \
	install-html-am install-info install-info-am install-man \
	install-man1 install-pdf install-pdf-am install-ps \
	install-ps-am install-strip installcheck installcheck-am \
	installdirs maintainer-clean maintainer-clean-generic \
	mostlyclean mostlyclean-generic mostlyclean-libtool pdf pdf-am \
	ps ps-am uninstall uninstall-am uninstall-dist_authoritiesDATA \
	uninstall-man uninstall-man1


################################################################
# localize and install process definition based on:
# SIPX_PROCESS_DEFS : the process definition file name without '-process.xml'

install-data-hook: \
	$(foreach proc,$(SIPX_PROCESS_DEFS),$(DESTDIR)$(SIPX_DATADIR)/process.d/$(proc)-process.xml)

$(foreach proc,$(SIPX_PROCESS_DEFS),$(DESTDIR)$(SIPX_DATADIR)/process.d/$(proc)-process.xml) \
	: $(DESTDIR)$(SIPX_DATADIR)/process.d/% : %
	$(INSTALL) -D -m 644 $< $@

$(foreach proc,$(SIPX_PROCESS_DEFS),$(proc)-process.xml) : % : %.in
	@echo "Localize process definition: $*"
	@$(LocalizeSipXconfig) $< > $@

.PHONY: $(foreach proc,$(SIPX_PROCESS_DEFS),$(proc)-process.xml)

################################################################
# Common pattern rules

# localize and syntax check a python file
%.py : %.py.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $< > $@
	chmod +x $@
	python -m py_compile $@

# localize and syntax check a perl file
%.pl : %.pl.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $< > $@
	chmod +x $@
	perl -wc $@

all: localize-sipx-scripts

.PHONY: localize-sipx-scripts
localize-sipx-scripts: $(foreach sipxScript,$(SCRIPT_TEMPLATES),$(sipxScript))

$(foreach sipxScript,$(SCRIPT_TEMPLATES),$(sipxScript)) : % : $(srcdir)/%.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) \
		-e 's,@OPENSSL\@,/usr/bin/openssl,g' \
		-e 's,@DATE_PARSE_ARGS\@,--date=,g' \
		$< > $@
	chmod +x $@

install-exec-hook : ssl-dirs install-sipx-scripts

.PHONY: ssl-dirs
ssl-dirs : $(DESTDIR)${prefix}/etc/sipxpbx/ssl $(DESTDIR)${prefix}/etc/sipxpbx/ssl/authorities

$(DESTDIR)${prefix}/etc/sipxpbx/ssl:
	$(INSTALL) -d -m 0755 $(DESTDIR)${prefix}/etc/sipxpbx/ssl

$(DESTDIR)${prefix}/etc/sipxpbx/ssl/authorities:
	$(INSTALL) -d -m 0755 $(DESTDIR)${prefix}/etc/sipxpbx/ssl/authorities

.PHONY: install-sipx-scripts
install-sipx-scripts: $(DESTDIR)$(bindir)/ssl-cert/check-cert.sh
install-sipx-scripts: $(foreach sipxScript,$(SCRIPT_TEMPLATES),$(DESTDIR)$(bindir)/ssl-cert/$(sipxScript))

$(foreach sipxScript,$(SCRIPT_TEMPLATES),$(DESTDIR)$(bindir)/ssl-cert/$(sipxScript)) : $(DESTDIR)$(bindir)/ssl-cert/% : %
	$(INSTALL) -D -m 755 $< $@

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
