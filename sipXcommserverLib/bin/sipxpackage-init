#/bin/sh

## Source function library.
. /home/joegen/sipxecs-bin/libexec/sipXecs/sipx-utils.sh || exit 1

# Ensure that sipxpackage is owned by root and
# that the UID permission bit is set.
if test -e /home/joegen/sipxecs-bin/libexec/sipXecs/sipxpackage
then
    is_file_owned_by_username /home/joegen/sipxecs-bin/libexec/sipXecs/sipxpackage "root"
    if [ $? -ne 0 ]
    then
        chown root /home/joegen/sipxecs-bin/libexec/sipXecs/sipxpackage
    fi
    if test ! -x /home/joegen/sipxecs-bin/libexec/sipXecs/sipxpackage -o ! -u /home/joegen/sipxecs-bin/libexec/sipXecs/sipxpackage
    then
        chmod 4755 /home/joegen/sipxecs-bin/libexec/sipXecs/sipxpackage
    fi
fi 
