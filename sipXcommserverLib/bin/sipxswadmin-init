#/bin/sh

## Source function library.
. /home/joegen/sipxecs-bin/libexec/sipXecs/sipx-utils.sh || exit 1

# Ensure that sipxswadmin is owned by root and
# that the UID permission bit is set.
if test -e /home/joegen/sipxecs-bin/libexec/sipXecs/sipxswadmin
then
    is_file_owned_by_username /home/joegen/sipxecs-bin/libexec/sipXecs/sipxswadmin "root"
    if [ $? -ne 0 ]
    then
        chown root /home/joegen/sipxecs-bin/libexec/sipXecs/sipxswadmin
    fi
    if test ! -x /home/joegen/sipxecs-bin/libexec/sipXecs/sipxswadmin -o ! -u /home/joegen/sipxecs-bin/libexec/sipXecs/sipxswadmin
    then
        chmod 4755 /home/joegen/sipxecs-bin/libexec/sipXecs/sipxswadmin
    fi
fi
