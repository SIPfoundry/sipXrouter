################################################################
# localize and install process definition based on:
# SIPX_PROCESS_DEFS : the process definition file name without '-process.xml'

install-data-hook: \
	$(foreach proc,$(SIPX_PROCESS_DEFS),$(DESTDIR)$(SIPX_DATADIR)/process.d/$(proc)-process.xml)

$(foreach proc,$(SIPX_PROCESS_DEFS),$(DESTDIR)$(SIPX_DATADIR)/process.d/$(proc)-process.xml) \
	: $(DESTDIR)$(SIPX_DATADIR)/process.d/% : %
	$(INSTALL) -D -m 644 $< $@

$(foreach proc,$(SIPX_PROCESS_DEFS),$(proc)-process.xml) : % : %.in
	@echo "Localize process definition: $*"
	@$(LocalizeSipXconfig) $< > $@

.PHONY: $(foreach proc,$(SIPX_PROCESS_DEFS),$(proc)-process.xml)

################################################################
# Common pattern rules

# localize and syntax check a python file
%.py : %.py.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $< > $@
	chmod +x $@
	python -m py_compile $@

# localize and syntax check a perl file
%.pl : %.pl.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $< > $@
	chmod +x $@
	perl -wc $@

install-data-hook : install-schema

.PHONY: install-schema
install-schema: $(foreach xsd,$(schemas) $(processed_schemas),$(DESTDIR)${datarootdir}/sipxecs/schema/$(xsd))

# Unprocessed schemas will be in $(srcdir).
$(foreach xsd,$(schemas),$(DESTDIR)${datarootdir}/sipxecs/schema/$(xsd)): $(DESTDIR)${datarootdir}/sipxecs/schema/% : $(srcdir)/%
	$(INSTALL) -D -m 644 $< $@

# Processed schemas will be in ".".
$(foreach xsd,$(processed_schemas),$(DESTDIR)${datarootdir}/sipxecs/schema/$(xsd)): $(DESTDIR)${datarootdir}/sipxecs/schema/% : %
	$(INSTALL) -D -m 644 $< $@

$(foreach xsd,$(processed_schemas),$(xsd)): % : $(srcdir)/%.in
	@echo "Localize $< -> $@"
	@$(LocalizeSipXconfig) $< > $@

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
